<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd <http://mybatis.org/dtd/mybatis-3-mapper.dtd> ">

<mapper namespace="com.springcamelmybatis.db.entity.ClaimInfoMapper">

<select id="getClaimInfo" resultType="ClaimInfo">
SELECT clm_id, typ_cde, clm_num, cde, comt_txt, mthd_cde 
FROM clm_t
WHERE clm_id = 1234567890
</select>

</mapper>
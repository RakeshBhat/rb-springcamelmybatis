<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- ================================================================== -->
	<!-- Type aliases that can be referenced from the mapper XML files. -->
	<!-- ================================================================== -->
	<typeAliases>
		<typeAlias alias="ClaimInfo" type="com.springcamelmybatis.db.entity.ClaimInfo" />
	</typeAliases>

	<!-- ================================================================== -->
	<!-- MyBatis Mapper XML resources -->
	<!-- ================================================================== -->

	<mappers>
		<mapper resource="mybatis/mappers/ClaimMapper.xml" />
	</mappers>

</configuration>